{% extends 'base.html.twig' %}

{% block title %}Association index{% endblock %}

{% block body %}
    <h1>Association index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Address</th>
                <th>Municipality</th>
                <th>Phone</th>
                <th>ContactNotes</th>
                <th>Website</th>
                <th>Email</th>
                <th>ServiceUsers</th>
                <th>Service</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for association in associations %}
            <tr>
                <td>{{ association.id }}</td>
                <td>{{ association.name }}</td>
                <td>{{ association.address }}</td>
                <td>{{ association.municipality }}</td>
                <td>{{ association.phone }}</td>
                <td>{{ association.contactNotes }}</td>
                <td>{{ association.website }}</td>
                <td>{{ association.email }}</td>
                <td>{{ association.serviceUsers }}</td>
                <td>{{ association.service }}</td>
                <td>
                    <a href="{{ path('association_show', {'id': association.id}) }}">show</a>
                    <a href="{{ path('association_edit', {'id': association.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="11">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('association_new') }}">Create new</a>
{% endblock %}
