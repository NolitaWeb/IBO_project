{% extends 'base.html.twig' %}

{% block title %}Association{% endblock %}

{% block body %}
    <h1>Association</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ association.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ association.name }}</td>
            </tr>
            <tr>
                <th>Address</th>
                <td>{{ association.address }}</td>
            </tr>
            <tr>
                <th>Municipality</th>
                <td>{{ association.municipality }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ association.phone }}</td>
            </tr>
            <tr>
                <th>ContactNotes</th>
                <td>{{ association.contactNotes }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ association.website }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ association.email }}</td>
            </tr>
            <tr>
                <th>ServiceUsers</th>
                <td>{{ association.serviceUsers }}</td>
            </tr>
            <tr>
                <th>Service</th>
                <td>{{ association.service }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('association_index') }}">back to list</a>

    <a href="{{ path('association_edit', {'id': association.id}) }}">edit</a>

    {{ include('association/_delete_form.html.twig') }}
{% endblock %}
